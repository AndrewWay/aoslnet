<SCRIPT TYPE="text/javascript">
function popup(mylink, windowname) {
  if (! window.focus)
    return true;
  var href;
  if (typeof(mylink) == 'string')
    href=mylink;
else href=mylink.href;
var myWindow = window.open(href, windowname, 'width=400,height=200,scrollbars=no');     
myWindow.document.write('<h3> Please enter the password to view content</h3><div data-role="popup" id="myPopup" class="ui-content" style="min-width:250px;"><div><label for="pswd" class="ui-hidden-accessible">Password:</label><input type="password" name="passw" id="pswd" placeholder="Password"><input type="submit" data-inline="true" value="Log in"></div></div>');
return false;
}
</SCRIPT>







